    <style>
        /* Container superior */
.ctn-superior{
    background-color:whitesmoke;
    width: 80%;
    height: 50vh;
    margin-top: 2%;
    display: flex;
    justify-content:space-evenly;
    align-items: center;
    border-radius: 5px;
}
.ctn-superior hr{
    background-color: #b4b3ac;
    height:40vh;
    width: 0.2px;
    border-radius: 80px;
}
.ctn-superior img{
    width: 50%;
    height:40vh;
    border-radius: 2px;
}
.btn-filtroTabla,
.btn-activarModal{
    background-color:#33363a;
    color: #e8e8e8d7;
    width: 80%;
    margin:5% 10%;
    border-radius: 80px;
}
.btn-filtroTabla:hover,
.btn-activarModal:hover{
    background-color:#e8e8e8d7;
    color: #33363a;
    transform: scale(1.1);
    box-shadow: 0px 5px 15px #33363a;
}
.ctn-superior .ctn-modals{
    width: 30%;
    height:40vh;
    display: flex;
    flex-flow: column wrap;
    align-items: center;
    justify-content: center;
}
.modal-content{
    background-color:#e8e8e8;
    border-radius: 18px;
}
.modal-content .modal-header{
    justify-content: center;
}

.modal .modal-footer button{
    background-color:#e8e8e8d7;
    color: black;
}
form .form-group{
    display: flex;
    justify-content: space-evenly;
}
.form-group label {
    width: 20%;
}

.form-group-vacuna option {
    width: 100%;
}

.form-group input{
    width: 60%;
}

.form-group textarea{
    resize: none;
}

form .btn-dark{
    background-color:#e8e8e8d7;
    color: black;
    margin-left: 40%;
}

/* Container inferior */

.ctn-inferior{
    background-color:whitesmoke;
    width: 80%;
    height: 40vh;
    margin-top:2%;
    display: flex;
    flex-wrap: wrap;
    justify-content:space-evenly;
    align-items: center;
    border-radius: 5px;
    margin-bottom:2% ;
}

.ctn-inferior .ctn-agendamiento{
    width: 35%;
    height: auto;
    display: flex;
    flex-direction: column;
    justify-content: space-around;

}
.ctn-inferior hr{
    background-color: #b4b3ac;
    height:30vh;
    width: 0.3px;
    border-radius: 80px;
}

.ctn-inferior .ctn-TablaSolictud{
    width: 60%;
    height: 30vh;
    align-items: center;
    overflow-y:scroll;
}
.ctn-inferior .ctn-TablaSolictud::-webkit-scrollbar {
    width: 8px;     
    height: 8px;  
}
.ctn-inferior .ctn-TablaSolictud::-webkit-scrollbar-thumb {
    background:#33363a;
    border-radius: 4px;
}

.ctn-TablaSolictud .table tbody ,
    .ctn-TablaSolictud .table thead th{
    text-align: center;
}

.ctn-TablaSolictud .table tbody .estado-icono{
    width: 20%;
    align-self: center;
}

.ctn-TablaSolictud .table .btn-Solicitud{
    background-color: #e8e8e8;
    border-radius: 80px;
    border:none;
}
.ctn-TablaSolictud .table .btn-Solicitud:hover{
    background-color: #e8e8e8;
    color: #33363a;
    transform: scale(1.1);
}


