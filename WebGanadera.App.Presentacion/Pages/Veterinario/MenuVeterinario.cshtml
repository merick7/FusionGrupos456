        <div class="container ctn-modals">
            <!-- Modal Registrar -->
            <button type="button" class="btn btn-activarModal" data-toggle="modal" data-target="#registrarVacuna">
                Registrar Vacuna
            </button>
            <div class="modal fade" id="registrarVacuna" tabindex="-1" role="dialog" aria-labelledby="registrarVacuna"
                aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="registrarVacunaTitle">Registrar Vacuna</h5>
                        </div>
                        <div class="modal-body">
                            <!-- formulario -->
                            <form>
                                <div class="form-group">
                                    <label for="nombre">Nombre</label>
                                    <input type="text" class="form-control" id="nombre" placeholder="Nombre" required>
                                </div>
                                <div class="form-group">
                                    <label for="descripcion">Descripción</label>
                                    <input type="text" class="form-control" id="descripcion" placeholder="  " required>
                                </div>
                                <div class="form-group">
                                    <label for="lote">Lote</label>
                                    <input type="text" class="form-control" id="lote" placeholder="0000" required>
                                </div>
                                <div class="form-group">
                                    <label for="fechaVencimiento">Fecha de Vencimiento</label>
                                    <input type="date" class="form-control" id="fechaVencimiento"
                                        placeholder="DD/MM/AAAA" required>
                                </div>
                                <div class="form-group">
                                    <label for="registroSanitario">Registro Sanitario</label>
                                    <input type="text" class="form-control" id="registroSanitario" placeholder="   " required>
                                </div>
                                <div class="form-group">
                                    <label for="laboratorio">Laboratorio</label>
                                    <input type="text" class="form-control" id="laboratorio" placeholder="   " required>
                                </div>
                                <button type="submit" class="btn btn-dark">Registrar</button>
                            </form>

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal Registrar -->

            <!-- Modal Modificar -->
            <button type="button" class="btn btn-activarModal" data-toggle="modal" data-target="#modificarVacuna">
