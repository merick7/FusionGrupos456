                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="modificarVacunaTitle">Modificar Vacuna</h5>
                        </div>
                        <div class="modal-body">
                            <!-- formulario -->
                            <form>
                                <div class="form-group mb-3">
                                    <label  for="select-vacuna-modificar">Seleccionar vacuna</label>
                                    <select class="custom-select" id="select-vacuna-modificar" required>
                                      <option value="nulo">Seleccione vacuna a modificar</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label for="nombre">Nombre</label>
                                    <input type="text" class="form-control" id="nombre" placeholder="Nombre" required>
                                </div>
                                <div class="form-group">
                                    <label for="descripcion">Descripción</label>
                                    <input type="text" class="form-control" id="descripcion" placeholder="  " required>
                                </div>
                                <div class="form-group">
                                    <label for="lote">Lote</label>
                                    <input type="text" class="form-control" id="lote" placeholder="0000" required>
                                </div>
                                <div class="form-group">
                                    <label for="fechaVencimiento">Fecha de Vencimiento</label>
                                    <input type="date" class="form-control" id="fechaVencimiento"
                                        placeholder="DD/MM/AAAA" required>
                                </div>
                                <div class="form-group">
                                    <label for="registroSanitario">Registro Sanitario</label>
                                    <input type="text" class="form-control" id="registroSanitario" placeholder="   " required>
                                </div>
                                <div class="form-group">
                                    <label for="laboratorio">Laboratorio</label>
                                    <input type="text" class="form-control" id="laboratorio" placeholder="   " required>
                                </div>
                                <button type="submit" class="btn btn-dark">Modificar</button>
                            </form>

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal Modificar-->

            <!-- Modal Eliminar -->
            <button type="button" class="btn btn-activarModal" data-toggle="modal" data-target="#eliminarVacuna">
                Eliminar Vacuna
            </button>
            <div class="modal fade" id="eliminarVacuna" tabindex="-1" role="dialog" aria-labelledby="eliminarVacuna"
                aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="eliminarVacunaTitle">Eliminar Vacuna</h5>
                        </div>
                        <div class="modal-body">
                            <!-- formulario -->
                            <form>
                                <div class="form-group mb-3">
                                    <label  for="select-ganado-modificar">Seleccionar ganado</label>
                                    <select class="custom-select" id="select-ganado-modificar" required>
                                      <option value="nulo">Seleccione ganado a modificar</option>
                                    </select>
                                </div>
                                <button type="submit" class="btn btn-dark">Eliminar</button>
                            </form>

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal ELiminar -->
        </div>
        <hr>
        <img src="https://i.ibb.co/CvNpwyL/Whats-App-Image-2021-10-18-at-00-57-41-2.jpg" alt="logo">
    </div>

    <div class="container-fluid ctn-inferior">
        <!-- Select Tipo Cita -->
        <div class="container ctn-agendamiento">
            <div class="input-group mb-3">
                <div class="input-group-prepend">
                    <label class="input-group-text" for="selectSolicitud">Solicitudes</label>
                </div>
                <select class="custom-select" id="selectSolicitud">
                    <option value="ninguna" selected>Seleccione Tipo de Solicitud </option>
                    <option value="vacuna">Vacunación</option>
                    <option value="tipoCita">Cita Médica</option>
                    <option value="pendiente">Estado pendiente</option>
                    <option value="enProceso">Estado en proceso</option>
