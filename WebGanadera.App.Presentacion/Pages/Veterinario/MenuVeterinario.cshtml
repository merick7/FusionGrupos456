                        <th scope="col">Estado</th>
                        <th scope="col">Accion</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Cita Medica</td>
                        <td>Andrea Erick</td>
                        <td><img src="https://i.ibb.co/LrY8RyK/Whats-App-Image-2021-10-18-at-9-04-38-AM.jpg" class="estado-icono" alt="pendiente"></td>
                        <td><button type="button" class="btn-Solicitud" data-toggle="modal" data-target="#SolicitudPendiente">
                            Detalles
                          </button></td>
                    </tr>
                    <tr>
                        <td>Vacunación</th>
                        <td>Stiven Alejandro</td>
                        <td><img src="https://i.ibb.co/m6m1912/Whats-App-Image-2021-10-18-at-9-04-38-AM-2.jpg" class="estado-icono" alt="proceso"></td>        
                        <td><button type="button" class="btn-Solicitud" data-toggle="modal" data-target="#SolicitudProceso">
                            Detalles
                          </button></td>
                      </tr>
                      <tr>
                        <td>Atención Médica</th>
                        <td>Javier Lina</td>
                        <td><img src="https://i.ibb.co/my34fvj/Whats-App-Image-2021-10-18-at-9-04-38-AM-1.jpg" class="estado-icono" alt="finalizado"></td>        
                        <td><button type="button" class="btn-Solicitud" data-toggle="modal" data-target="#SolicitudFinalizado">
                            Detalles
                          </button></td>
                      </tr>
                </tbody>
            </table>
        </div>
    </div>



