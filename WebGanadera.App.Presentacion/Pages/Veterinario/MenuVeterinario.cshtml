                </select>
            </div>
            <!-- Boton Filtro-->
            <button type="button" class="btn btn-filtroTabla">
                Mostrar
            </button>
        </div>

        <hr>

        <!-- Tabla de Estado -->
        <div class="container ctn-TablaSolictud">
            <table class="table table-dark">
                <thead>
                    <tr>
                        <th scope="col">Tipo de atención</th>
